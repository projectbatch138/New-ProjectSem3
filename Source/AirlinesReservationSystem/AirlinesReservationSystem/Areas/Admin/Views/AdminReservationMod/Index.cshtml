@model IEnumerable<AirlinesReservationSystem.ReservationMod>

@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ReservationModName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ReservationModName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ReservationModId }) |
                @Html.ActionLink("Details", "Details", new { id = item.ReservationModId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ReservationModId })
            </td>
        </tr>
    }

</table>
